# OpenAI Configuration
OPENAI_API_KEY=your_openai_api_key_here
OPENAI_MODEL=gpt-4-turbo-preview
OPENAI_EMBEDDING_MODEL=text-embedding-3-small

# Mem0 Configuration
MEM0_API_KEY=your_mem0_api_key_here
MEM0_PROJECT_ID=your_project_id

# Vector Database
VECTOR_DB_TYPE=chroma
CHROMA_PERSIST_DIR=./chroma_db
PINECONE_API_KEY=your_pinecone_key
PINECONE_ENVIRONMENT=your_environment
PINECONE_INDEX_NAME=aware-ai

# Server Configuration
BACKEND_URL=http://localhost:8000
FRONTEND_URL=http://localhost:3000
CORS_ORIGINS=http://localhost:3000,http://localhost:3001

# Memory Configuration
MEMORY_COMPRESSION_THRESHOLD=50
MEMORY_SUMMARY_INTERVAL=10
MAX_CONTEXT_TOKENS=8000

# Fine-tuning
FINE_TUNING_ENABLED=true
FINE_TUNING_DATA_DIR=./fine_tuning_data

# Embeddings
USE_CUSTOM_EMBEDDINGS=false

# Supabase (for persistent conversation threads)
SUPABASE_URL=https://your-project.supabase.co
SUPABASE_KEY=your_supabase_anon_key
